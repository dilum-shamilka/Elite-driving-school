<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.controller.ReceptionLoginController">
    <children>
        <Label layoutX="146.0" layoutY="50.0" text="Reception Login">
            <font>
                <Font name="System Bold" size="36.0" />
            </font>
        </Label>
        <Label layoutX="120.0" layoutY="150.0" text="Username:">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="txtUsername" layoutX="240.0" layoutY="148.0" prefHeight="30.0" prefWidth="250.0" />
        <Label layoutX="120.0" layoutY="220.0" text="Password:">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <PasswordField fx:id="txtPassword" layoutX="240.0" layoutY="218.0" prefHeight="30.0" prefWidth="250.0" />
        <Button fx:id="btnLogin" layoutX="270.0" layoutY="300.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="40.0" prefWidth="120.0" text="Login">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>